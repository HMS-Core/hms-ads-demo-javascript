<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huawei.hms.ads.jsdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.huawei.hms.ads.jsdemo.AdSampleApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.huawei.hms.ads.jsdemo.MainActivity"
            android:configChanges="orientation|screenSize"
            android:exported="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.huawei.hms.ads.jsdemo.BannerActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.huawei.hms.ads.jsdemo.NativeActivity"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.huawei.hms.ads.jsdemo.RewardActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.huawei.hms.ads.jsdemo.InterstitialActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.huawei.hms.ads.jsdemo.ProtocolActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.huawei.hms.ads.jsdemo.ConsentActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>